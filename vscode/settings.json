{
    "telemetry.enableTelemetry": false,
    "extensions.autoUpdate": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.iconTheme": "solarized",
    "workbench.colorTheme": "Rick's Solarized Dark",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.enableExperiments": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.minimap.renderCharacters": false,
    "editor.detectIndentation": false,
    "editor.smoothScrolling": true,
    "editor.minimap.maxColumn": 90,
    "editor.fontFamily": "Hack NF, Hack, Fira Code, monospace",
    "editor.fontSize": 12,
    "editor.rulers": [ 80 ],
    "editor.formatOnType": false,
    "editor.folding": false,
    "search.showLineNumbers": true,
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.html": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/*.pyc": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.project": true,
        "**/.pydevproject": true,
        "**/.settings": true,
        "**/.ccls-cache": true
    },
    "todo-tree.tags": [
        "TODO",
        "FIXME",
        "NOTE",
        "#warning",
        "#error",
        "xxRSPxx"
    ],
    "todo-tree.showInExplorer": false,
    "todo-tree.defaultHighlight": {
        "foreground": "#85a900",
        "background": "#001e29",
        "iconColour": "#85a900",
        "type": "text"
    },
    "todo-tree.customHighlight": {
        "TODO": {
            "foreground": "#b58900",
            "iconColour": "#b58900",
        },
        "FIXME": {
            "icon": "bug",
            "iconColour": "#cb4b16",
            "foreground": "#cb4b16"
        },
        "NOTE": {
            "icon": "info"
        },
        "#warning": {
            "icon": "alert",
            "foreground": "#d33682",
            "iconColour": "#d33682"
        },
        "#error": {
            "icon": "stop",
            "foreground": "#dc322f",
            "iconColour": "#dc322f"
        },
        "xxRSPxx": {
            "icon": "issue-opened",
            "iconColour": "#6c71c4",
            "foreground": "#6c71c4"
        }
    },
    "todo-tree.expanded": true,
    "doxdocgen.c.commentPrefix": "/// ",
    "doxdocgen.c.firstLine": "///-----------------------------------------------------------------------------",
    "doxdocgen.c.lastLine": "///------------------------------------------------------------------------------",
    "doxdocgen.c.triggerSequence": "///-",
    "doxdocgen.file.fileTemplate": "@file {name}",
    "doxdocgen.generic.authorTag": "@author Rick St Pierre",
    "doxdocgen.generic.authorName": "Rick St Pierre",
    "gitExtensions.exe.path": "C:/Users/Rst.Pie/scoop/apps/gitextensions-dev/current/gitextensions/gitextensions.exe",
    "vim.overrideCopy": true,
    "vim.useCtrlKeys": true,
    "vim.highlightedyank.enable":true,
