{
    "telemetry.enableTelemetry": false,
    "extensions.autoUpdate": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.iconTheme": "solarized",
    "workbench.colorTheme": "Rick's Solarized Dark",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.enableExperiments": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.minimap.renderCharacters": false,
    "editor.detectIndentation": false,
    "editor.smoothScrolling": true,
    "editor.minimap.maxColumn": 90,
    "editor.fontFamily": "Hack NF, Hack, Fira Code, monospace",
    "editor.fontSize": 12,
    "editor.rulers": [ 80 ],
    "editor.formatOnType": false,
    "editor.folding": false,
    "search.showLineNumbers": true,
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.html": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/*.pyc": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.project": true,
        "**/.pydevproject": true,
        "**/.settings": true,
        "**/*_boot": true,
        "**/*_exe": true,
        "**/*_ramboot": true
    },
    "todo-tree.tags": [
        "TODO",
        "FIXME",
        "NOTE",
        "#warning",
        "#error",
        "xxRSPxx"
    ],
    "todo-tree.showInExplorer": false,
    "todo-tree.defaultHighlight": {
        "foreground": "#85a900",
        "background": "#001e29",
        "iconColour": "#85a900",
        "type": "text"
    },
    "todo-tree.customHighlight": {
        "TODO": {
            "foreground": "#b58900",
            "iconColour": "#b58900",
        },
        "FIXME": {
            "icon": "bug",
            "iconColour": "#cb4b16",
            "foreground": "#cb4b16"
        },
        "NOTE": {
            "icon": "info"
        },
        "#warning": {
            "icon": "alert",
            "foreground": "#d33682",
            "iconColour": "#d33682"
        },
        "#error": {
            "icon": "stop",
            "foreground": "#dc322f",
            "iconColour": "#dc322f"
        },
        "xxRSPxx": {
            "icon": "issue-opened",
            "iconColour": "#6c71c4",
            "foreground": "#6c71c4"
        }
    },
    "todo-tree.expanded": true,
    "doxdocgen.c.commentPrefix": "/// ",
    "doxdocgen.c.firstLine": "///-----------------------------------------------------------------------------",
    "doxdocgen.c.lastLine": "///------------------------------------------------------------------------------",
    "doxdocgen.c.triggerSequence": "///-",
    "doxdocgen.file.fileTemplate": "@file {name}",
    "doxdocgen.generic.authorTag": "@author Rick St Pierre",
    "doxdocgen.generic.authorName": "Rick St Pierre",
    "C_Cpp.clang_format_path": "${env:USERPROFILE}/scoop/apps/LLVM/current/bin/clang-format.exe",
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "vim.overrideCopy": true,
    "vim.highlightedyank.color": "rgba(211, 54, 130, 0.5)",
    "vim.highlightedyank.duration": 500,
    "vim.highlightedyank.enable": true,
    "breadcrumbs.enabled": true,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "Hack NF, Hack",
    "terminal.integrated.shell.windows": "cmd.exe",
    "terminal.integrated.shellArgs.windows": ["/k", "C:/Users/rsp/scoop/apps/conemu/current/conemu/cmdinit.cmd"],
    "terminal.integrated.windowsEnableConpty": true,
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": false
    },
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.inputValidation": "off",
    "git.confirmSync": false,
    "git.enableCommitSigning": true,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "markdown",
        "plaintext",
        "restructuredtext",
        "text",
        "typescript",
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.ignoreRegExpList": [
        "/[A-Z]{2,}/g",
        "/cr[\\w]*_[\\w]*/g"
    ],
    "cortex-debug.JLinkGDBServerPath": "C:/Program Files (x86)/SEGGER/JLink_V640/JLinkGDBServerCL.exe",
    "debug.toolBarLocation": "docked",
    "cquery.launch.command":"C:/Users/rsp/Documents/projects/cquery/build/release/bin/cquery.exe",
    "cquery.launch.args":["-fno-ms-extensions","-fno-ms-compatibility","-fno-delayed-template-parsing"],
    "cquery.cacheDirectory": "C:/Users/rsp/.cquery-cache",
    "cquery.highlighting.enabled.types": true,
    "cquery.highlighting.enabled.freeStandingFunctions": true,
    "cquery.highlighting.enabled.memberFunctions": true,
    "cquery.highlighting.enabled.freeStandingVariables": true,
    "cquery.highlighting.enabled.memberVariables": true,
    "cquery.highlighting.enabled.namespaces": true,
    "cquery.highlighting.enabled.macros": true,
    "cquery.highlighting.enabled.enums": true,
    "cquery.highlighting.enabled.typeAliases": true,
    "cquery.highlighting.enabled.enumConstants": true,
    "cquery.highlighting.enabled.staticMemberFunctions": true,
    "cquery.highlighting.enabled.parameters": true,
    "cquery.highlighting.enabled.templateParameters": true,
    "cquery.highlighting.enabled.staticMemberVariables": true,
    "cquery.highlighting.enabled.globalVariables": true,
    "cquery.misc.status": "detailed",
}
